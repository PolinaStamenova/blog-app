<%# <%= render @users%>
<ul>
  <% @users.each do |user|%>
    <%= link_to user_path(user) do%>
      <li>
        <%= image_tag user.photo %>
        <div>
          <p><%= user.name %></p>
          <p>Number of posts:<%= user.posts.size %></p>
        </div>
      </li>
    <%end%>
  <%end%>
</ul>

